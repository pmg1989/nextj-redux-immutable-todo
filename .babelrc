{
  "presets": [
    "env",
    "next/babel",
    "react",
    "es2015",
    "stage-0",
    "stage-1"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"]
    }]
  ],
  "env": {
    "development": {
      "plugins": ["inline-dotenv"]
    },
    "production": {
      "plugins": ["transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-inline-environment-variables"
      ]
    }
  }
}
