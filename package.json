{
  "name": "nextjs-redux-immutable-todo",
  "version": "1.0.0",
  "main": "server/index.js",
  "description": "Next.js redux starter with Express, Redux, SASS, and Jest.",
  "scripts": {
    "build": "NODE_ENV=production next build",
    "test:ci": "jest --maxWorkers=2 --ci",
    "test": "jest --watch",
    "dev": "nodemon -w server server/index.js",
    "start": "NODE_ENV=production node server/index.js",
    "clean": "rimraf node_modules/.cache .next",
    "lint": "standard --verbose | snazzy",
    "lint-fix": "standard --fix --verbose | snazzy"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "lru-cache": "^4.1.1",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.3.0",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "styled-jsx-css-loader": "^0.3.0"
  },
  "devDependencies": {
    "babel-plugin-inline-dotenv": "^1.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.1.2",
    "nodemon": "^1.12.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": "8.9.3",
    "npm": "5.5.1",
    "yarn": "1.3.2"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/test/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/jest.setup.js"
  }
}
