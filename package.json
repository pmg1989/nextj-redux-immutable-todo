{
  "name": "nextjs-redux-immutable-todo",
  "version": "1.0.0",
  "main": "server/index.js",
  "description": "Next.js redux starter with Express, Redux, less, and Immutable.",
  "scripts": {
    "clean": "rimraf node_modules/.cache .next", 
    "dev": "yarn run clean && REACT_APP_API_ENV=dev node server/index.js",
    "dev:staging": "yarn run clean && REACT_APP_API_ENV=staging node server/index.js",
    "dev:release": "yarn run clean && REACT_APP_API_ENV=prod node server/index.js",
    "build:dev": "yarn run clean && NODE_ENV=production REACT_APP_API_ENV=dev next build",
    "start:dev": "NODE_ENV=production REACT_APP_API_ENV=dev node server/index.js",
    "build:staging": "yarn run clean && NODE_ENV=production REACT_APP_API_ENV=staging next build",
    "start:staging": "NODE_ENV=production REACT_APP_API_ENV=staging node server/index.js",
    "build:release": "yarn run clean && NODE_ENV=production REACT_APP_API_ENV=prod next build",
    "start": "NODE_ENV=production REACT_APP_API_ENV=prod node server/index.js",
    "build:release:analyze": "yarn run clean && NODE_ENV=production REACT_APP_API_ENV=prod ANALYZE=1 next build",
    "lint": "eslint --fix --ext .js src server pages",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@zeit/next-less": "^0.1.1",
    "antd-mobile": "^2.1.6",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "less": "^3.0.1",
    "lru-cache": "^4.1.1",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.3.0",
    "normalize.css": "^7.0.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "svg-sprite-loader": "0.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "husky": "^0.12.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack-bundle-analyzer": "^2.9.1"
  }
}
